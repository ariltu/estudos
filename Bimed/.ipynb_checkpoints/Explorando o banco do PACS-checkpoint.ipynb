   "source": [
    "ano2016 = pd.DataFrame(estudo[2016])\n",
    "totais2016 = [0] * len(modalidades)\n",
    "totais2016_mensais = [0] * 12\n",
    "for mes in ano2016:\n",
    "    for i in range(len(modalidades)):\n",
    "        totais2016[i] += ano2016[mes][i]\n",
    "        totais2016_mensais[mes - 1] += ano2016[mes][i]\n",
